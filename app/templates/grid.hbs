<div class="well well-small">
    <p class="lead">{{title}}</p>
</div>
<table class="table table-striped table-bordered table-hover table-condensed">
    <thead>
    <tr class="centered-header">
        {{#each headers}}
        <td>{{.}}</td>
        {{/each}}
    </tr>
    </thead>
    <tbody>
    {{#each rows}}
    <tr>
        {{#each values}}
        <td>{{.}}</td>
        {{/each}}
    </tr>
    {{/each}}
    </tbody>
</table>